    "def maxim(a,b,c):\n",
    "    if a>b and a>c:\n",
    "       max=a\n",
    "    elif b>a and b>c:\n",
    "        max=b\n",
    "    else:\n",
    "        max=c\n",
    "    return max\n",
    "\n",
    "a=int(input())\n",
    "b=int(input())\n",
    "c=int(input())\n",
    "print(maxim(a,b,c))"
